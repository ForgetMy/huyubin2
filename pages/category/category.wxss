.navs{
  display: flex;
}
.left{
  height: calc(100vh - 68px);
  width: 150rpx;
  text-align: center;
}
.navs-r{
  flex:1;
}
.cont{
  width: 100%;
  height: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.top{
  width: 100%;
  font-size:28rpx;
  line-height: 116rpx;
  text-align: center;
}
.goods{
  width: 100%;
  height: calc(100vh - 58px - 108px);
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: flex-start;
}
.goods::after{
  width: 31%;
  content: "";
  display: block;
}
.good{
  width: 31%;
  height: 280rpx;
  text-align: center;
}
.good image{
  width: 96%;
  height: 170rpx;
}